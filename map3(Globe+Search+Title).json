{
    "id": "com.sap.custom.map4",
    "version": "3.0.0",
    "name": "Map Sample 4",
    "description": "Map widget",
    "newInstancePrefix": "geoMap",
    "icon": "data:image/png;base64,",
    "vendor": "sap",
    "eula": "",
    "license": "",
    "imports": [ "input-controls" ],
    "webcomponents": [
      {
        "kind": "main",
        "tag": "com-sap-custom-geomap",
        "url": "https://testsap.github.io/map3.js",
        "integrity": "",
        "ignoreIntegrity": true
      },
      {
        "kind": "builder",
        "tag": "com-sap-custom-geomap-builder",
        "url": "https://testsap.github.io/geomap_aps_new_2.js",
        "integrity": "",
        "ignoreIntegrity": true
      }
    ],
    "properties": {
      "metadata": {
        "type": "string",
        "description": "For internal use",
        "default": ""
      },
      "servicelevel": {
        "type": "integer",
        "description": "Electrical NODISCONT Service Level",
        "default": 0
      },
      "portalId":{
         "type":"string",
         "description":"portal ID",
         "default":""
      },
      "titleId":{
         "type":"string",
         "description":"title ID",
         "default":""
      }
    },
    "methods": {
      "setServiceLevel": {
        "description": "Set the customer Service Level",
        "parameters": [
          {
            "name": "service",
            "type": "integer",
            "description": "ServiceLevel"
          }
        ],
        "body": "this.servicelevel = service;"
      },
      
      "getPortalId":{
         "returnType":"string",
         "description":"Portal Id",
         "parameters":[
            
         ],
         "body":"return this.portalId;"
      },
      "setPortalId":{
         "description":"Portal Id",
         "parameters":[
            {
               "name":"pid",
               "type":"string",
               "description":"Set New Portal Id"
            }
         ],
         "body":"this.portalId = pid;"
      },
      
      "getTitleId":{
         "returnType":"string",
         "description":"Title Id",
         "parameters":[
            
         ],
         "body":"return this.portalId;"
      },
      "setTitleId":{
         "description":"Title Id",
         "parameters":[
            {
               "name":"titleid",
               "type":"string",
               "description":"Set New Portal Id"
            }
         ],
         "body":"this.titlelId = titleid;"
      }
    },
    "events": {
      "onStart": {
        "description": "onStart events trigger"
      }
    }
  }